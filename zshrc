. ~/.zsh/config
. ~/.zsh/completion
. ~/.zsh/aliases
[[ -s ~/.rvm/scripts/rvm ]]               && . ~/.rvm/scripts/rvm
[[ -s /usr/local/rvm/scripts/rvm ]]       && . /usr/local/rvm/scripts/rvm
[[ -s ~/.tmuxinator/scripts/tmuxinator ]] && . ~/.tmuxinator/scripts/tmuxinator
[[ -f `brew --prefix`/etc/autojump ]]     && . `brew --prefix`/etc/autojump
